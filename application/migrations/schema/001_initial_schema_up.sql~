CREATE TABLE IF NOT EXISTS `auth_group` (`id` int(11) NOT NULL AUTO_INCREMENT, `name` varchar(200) NOT NULL, `auth_module` text NOT NULL, `status` tinyint(1) DEFAULT '0', `updated_at` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP, PRIMARY KEY (`id`)) ENGINE=InnoDB;
INSERT INTO `auth_group` (`id`, `name`, `auth_module`, `status`, `updated_at`) VALUES (1, 'Administrator', '["all","1","5","1","6","1","7","1","2","8","2","20","2","3","24","3","4","16","4","51","16","4","50","16","4","49","16","4","55","16","4","26","4","37","26","4","36","26","4","35","26","4","29","26","4","28","26","4","21","4","30","21","4","31","21","4","34","21","4","33","21","4","32","21","4","17","4","44","17","4","43","17","4","42","17","4","48","17","4","47","17","4","18","4","52","18","4","41","18","4","53","18","4","54","18","4"]', 1, '2014-09-17 04:59:22');
CREATE TABLE IF NOT EXISTS `auth_users` (`pkUserId` int(10) unsigned NOT NULL AUTO_INCREMENT, `username` varchar(50) NOT NULL, `group` varchar(20) DEFAULT NULL COMMENT 'VALUE: admin | operator | client | buyer', `password` varchar(40) NOT NULL, `last_login_datetime` datetime DEFAULT NULL,`last_login_ip` varchar(50) DEFAULT NULL,`active` tinyint(1) NOT NULL DEFAULT '1',`fullname` varchar(50) DEFAULT NULL,`email` varchar(100) DEFAULT NULL,`perm_dashboard_view` tinyint(1) DEFAULT '0',`perm_merchandise_view` tinyint(1) DEFAULT NULL, PRIMARY KEY (`pkUserId`)) ENGINE=InnoDB;
